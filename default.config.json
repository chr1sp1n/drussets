{
	"path":{
		"temp": "./.tmp",
		"dev": 	"../themes/custom/theme_name/libraries",
		"dist": "../themes_dist/custom/theme_name/libraries"
	},
	"assets":{
		"path": "./src",
		"libraries": "/libraries"
	},
	"css":{
		"src": "./src/libraries",
		"sass":{
			"exclude": []
		}
	},
	"js":{
		"src": 	"./src/libraries",
		"babel":{
			"config":{
				"presets": [
					"@babel/env", {}
				]
			},
			"exclude": [
				"jquery/**/*.js"
			]
		}
	},
	"browser":{
		"sync": true,
		"config": {
			"browser": "google chrome",
			"reloadDebounce": 500
		}
	}
}